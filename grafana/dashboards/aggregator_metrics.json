{
  "uid": "aggregator-metrics",
  "title": "Aggregator Service Metrics",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "graph",
      "title": "HTTP Requests Total",
      "datasource": "Prometheus",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 0 },
      "lines": true,
      "lineWidth": 2,
      "targets": [
        {
          "expr": "http_requests_total",
          "legendFormat": "Total HTTP Requests",
          "refId": "A"
        }
      ],
      "xaxis": { "mode": "time" }
    },
    {
      "id": 2,
      "type": "graph",
      "title": "Aggregator Packets Total",
      "datasource": "Prometheus",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 9 },
      "lines": true,
      "lineWidth": 2,
      "targets": [
        {
          "expr": "aggregator_packets_total",
          "legendFormat": "Processed Packets",
          "refId": "A"
        }
      ],
      "xaxis": { "mode": "time" }
    }
  ]
}
