{
  "id": null,
  "uid": "aggregator",
  "title": "Aggregator Metrics",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "tags": ["aggregator", "observability"],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Generator Packets Total",
      "datasource": null,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {"refId": "A", "expr": "aggregator_packets_total"}
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single", "sort": "none"}
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Generator Packets /s",
      "datasource": null,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {"refId": "A", "expr": "rate(aggregator_packets_total[5m])"}
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single", "sort": "none"}
      }
    },
